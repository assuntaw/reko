<!--
  THIS PARTIAL TAKES ONE INPUT PARAMETER
  "feedbacks" which is an array of feedback instances
  renders a table where each row is a feedback instance!
-->
<table class="table table-sm">
  <tr>
    <th>id</th>
    <th>content</th>
    <th>created_at</th>
    <th>updated_at</th>
    <!-- <th colspan="3">admin</th> -->
  </tr>
  <% feedbacks.each do |feedback| %>
    <tr>
      <td><%= feedback.id %></td>
      <td><%= feedback.content %></td>
      <td><%= feedback.created_at %></td>
      <td><%= feedback.updated_at %></td>
    </tr>
  <% end %>
</table>
